def PrAgivenB[a](d: Distribution[a], A: aâ†’ð”¹, B: aâ†’ð”¹){
	return expectation(infer((){
		x := sample(d);
		observe(B(x));
		return A(x);
	}));
}

def main(X,Y){
	joint := infer((){
		x := uniform(0,1);
		y := x^2+uniform(0,1);
		return (x,y);
	});
	return PrAgivenB(joint,(x,y)=>x<X/+0.5+/,(x,y)=>y>Y);
}
