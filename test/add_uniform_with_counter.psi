def main(){
    x := 0:ℝ; // TODO: this annotation should not be necessary
    y := 0;
    repeat 2{
        cur := uniform(0,1);
        x = x + cur;
        if cur<1/2 { y+=1; }
    }
    //z:=x==y;
    observe(y==1);
    return x; // expected: (-2·x+2)·[-3/2+x≤0]·[-x+1≠0]·[-x+1≤0]·λ[x]+-2·[-1+x≤0]·[-x+1/2≤0]·[-x+1≠0]·λ[x]+-2·[-3/2+x≤0]·[-x+1≤0]·x·λ[x]+4·[-1+x≤0]·[-x+1/2≤0]·[-x+1≠0]·x·λ[x]+4·[-3/2+x≤0]·[-x+1≤0]·λ[x]
}
