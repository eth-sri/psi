def q(b: ℝ[]){
	//return p(b);
	tot := 0;
	for i in [0..n){
		tot+=b[i];
	}
	for i in [0..n){
		if tot<n/2{
			if !b[i]{
				return n-1-i;
			}
		}else{
			if b[i]{
				return n-1-i;
			}
		}
	}
