
def newDistr(w){
    (x,y,z) := sampleFrom("(x,y,z;w) => [-1<=x]*[x<=1]*[-1<=y]*[y<=1]*|x|*|y|*lambda[x]*lambda[y]*delta(w)[z]",w);
    return (x,y,z);
}

