def multiGauss(Î¼: â„[], Î£: â„[][]){
	def mmv(A: â„[][], b: â„[])pure{
		r:=array(A.length,0.0);
		for i in [0..A.length){
			for j in [0..b.length){
				r[i]+=A[i][j]*b[j];
			}
		}
		return r;
	}
	def avv(a: â„[], b: â„[])pure{
		assert(a.length==b.length);
		r:=array(a.length,0.0);
		for i in [0..a.length){
			r[i]=a[i]+b[i];
		}
		return r;
	}
	def dot(a: â„[], b: â„[])pure{
		assert(a.length==b.length);
		r:=0.0;
		for i in [0..a.length){
			r+=a[i]*b[i];
		}
		return r;
	}
	def cholesky(A: â„[][])pure{
		for i in [0..A.length){
			assert(A[i].length==A.length);
			for j in [0..A[i].length){
				assert(A[i][j]==A[j][i]);
			}
		}
		L := array(A.length,array(A.length,0.0));
		for i in [0..A.length){
			for j in [0..i+1){
				t := dot(L[i][0..j],L[j][0..j]);
				if i == j {
					assert(A[i][j]>=t);
					L[i][j] = (A[i][j]-t)^(1/2)
				}else if L[j][j]!=0{
					L[i][j] = (A[i][j]-t)/L[j][j];
				}
			}
		}
		return L;
	}
	r := array(Î¼.length,0.0);
	for i in [0..r.length){
		r[i] = gauss(0,1);
	}
	return avv(mmv(cholesky(Î£),r),Î¼);
}
